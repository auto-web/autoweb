$TTL        3600
@       IN      SOA     {{ configs.domain_hostname }}. support.wellhosted.ch. (
                        2018090101       ; serial, todays date + todays serial #
                        7200              ; refresh, seconds
                        540              ; retry, seconds
                        604800              ; expire, seconds
                        86400 )            ; minimum, seconds
;

{% if configs.server_ipv4 != None and configs.server_ipv4 != '' %}
pma.{{ configs.domain_users }}. 3600 A      {{ configs.server_ipv4 }}
{% endif %}

{% if configs.server_ipv6 != None and configs.server_ipv6 != '' %}
pma.{{ configs.domain_users }}. 3600 AAAA    {{ configs.server_ipv6 }}
{% endif %}

pma.{{ configs.domain_users }}. 3600      NS        {{ configs.domain_hostname }}.


